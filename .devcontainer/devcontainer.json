{
	"name": "DDExt-Open-WebUI",
	"build": { 
		"context": "..",
		"dockerfile": "Dockerfile"
	},
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {},
		"ghcr.io/audacioustux/devcontainers/common-utils-extras:1": {},
		"ghcr.io/schlich/devcontainer-features/powerlevel10k:1": {},
		"ghcr.io/devcontainers/features/docker-outside-of-docker:1": {},
		"ghcr.io/devcontainers/features/git:1": {},
		"ghcr.io/devcontainers/features/github-cli:1": {},
		"ghcr.io/devcontainers-extra/features/gitomatic:1": {},
		"ghcr.io/devcontainers-extra/features/gitsign-credential-cache:1": {},
		"ghcr.io/devcontainers/features/go:1": {},
		"ghcr.io/devcontainers/features/node:1": {},
		"ghcr.io/devcontainers-extra/features/typescript:2": {},
		"ghcr.io/goldsam/dev-container-features/cue-lang:1": {},
		"ghcr.io/warrenbuckley/codespace-features/sqlite:1": {},
		"ghcr.io/devcontainers-extra/features/tailscale:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-windows-ai-studio.windows-ai-studio",
				"formulahendry.code-runner",
				"ms-azuretools.vscode-docker",
				"dbaeumer.vscode-eslint",
				"shyykoserhiy.git-autoconfig",
				"donjayamanne.githistory",
				"howardzuo.vscode-git-tags",
				"mhutchie.git-graph",
				"docker.labs-ai-tools-vscode",
				"ms-vscode.makefile-tools",
				"esbenp.prettier-vscode",
				"Gruntfuggly.todo-tree",
				"qcz.text-power-tools",
				"redhat.vscode-yaml",
				"Tailscale.vscode-tailscale",
				"DavidAnson.vscode-markdownlint",
				"golang.go",
				"eliostruyf.vscode-front-matter-beta",
				"mindaro-dev.file-downloader",
				"PeterSchmalfeldt.explorer-exclude",
				"docker.docker-vscode-extension",
				"ms-vscode-remote.vscode-remote-extensionpack",
				"ms-vscode.vscode-speech",
				"tomoki1207.pdf",
				"bierner.github-markdown-preview",
				"github.vscode-github-actions",
				"oderwat.indent-rainbow",
				"ms-python.python",
				"cweijan.vscode-redis-client",
				"foxundermoon.shell-format",
				"jock.svg",
				"tilt-dev.tiltfile",
				"XuangeAha.vsmarketplace-badges"
			]
		}
	},

	// Features to add to the dev container. More info: https://containers.dev/features.
	// "features": {},

	// Use 'forwardPorts' to make a list of ports inside the container available locally.
	// "forwardPorts": [],

	// Use 'postCreateCommand' to run commands after the container is created.
	// "postCreateCommand": "uname -a",

	// Configure tool-specific properties.
	// "customizations": {},

	// Uncomment to connect as root instead. More info: https://aka.ms/dev-containers-non-root.
	"remoteUser": "root"
}
